      <param name="COUNT" displayname="Number of Values" type="Optional" direction="Input">
        <dialogReference>&lt;p&gt;Number of Values&lt;/p&gt;&lt;p&gt;Grid, input, optional&lt;/p&gt;&lt;p&gt;optional&lt;/p&gt;</dialogReference>
