  <tool name="tool_21" displayname="Grid Histogram">
    <parameters>
      <param name="GRID" displayname="Grid" type="Required" direction="Input">
        <dialogReference>&lt;p&gt;Grid&lt;/p&gt;&lt;p&gt;Grid, input&lt;/p&gt;</dialogReference>
      </param>
      <param name="HISTOGRAM" displayname="Histogram" type="Required" direction="Output">
        <dialogReference>&lt;p&gt;Histogram&lt;/p&gt;&lt;p&gt;Table, output&lt;/p&gt;</dialogReference>
      </param>
      <param name="CLASSIFY" displayname="Classification" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Classification&lt;/p&gt;&lt;p&gt;Choice&lt;/p&gt;&lt;p&gt;Available Choices:&lt;/p&gt;&lt;p&gt;[0] value range and number of classes&lt;/p&gt;&lt;p&gt;[1] lookup table&lt;/p&gt;&lt;p&gt;Default: 0&lt;/p&gt;</dialogReference>
