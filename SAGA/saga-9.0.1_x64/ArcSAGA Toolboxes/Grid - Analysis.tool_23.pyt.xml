  <tool name="tool_23" displayname="Coverage of Categories">
    <parameters>
      <param name="CLASSES" displayname="Categories" type="Required" direction="Input">
        <dialogReference>&lt;p&gt;Categories&lt;/p&gt;&lt;p&gt;Grid, input&lt;/p&gt;</dialogReference>
      </param>
      <param name="COVERAGES" displayname="Coverages" type="Required" direction="Output">
        <dialogReference>&lt;p&gt;Coverages&lt;/p&gt;&lt;p&gt;Grid list, output&lt;/p&gt;</dialogReference>
      </param>
      <param name="LUT" displayname="Classification" type="Optional" direction="Input">
        <dialogReference>&lt;p&gt;Classification&lt;/p&gt;&lt;p&gt;Table, input, optional&lt;/p&gt;&lt;p&gt;optional&lt;/p&gt;</dialogReference>
      </param>
      <param name="LUT_VAL" displayname="Value" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Value&lt;/p&gt;&lt;p&gt;Table field&lt;/p&gt;&lt;p&gt;The class value or - in combination with value 2 - the minimum/maximum value specifying a value range.&lt;/p&gt;</dialogReference>
