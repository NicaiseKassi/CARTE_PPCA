  <tool name="tool_6" displayname="World File from Flight and Camera Settings">
    <parameters>
      <param name="EXTENT" displayname="Extent" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Extent&lt;/p&gt;&lt;p&gt;Shapes, output, optional&lt;/p&gt;&lt;p&gt;optional&lt;/p&gt;</dialogReference>
      </param>
      <param name="FILE" displayname="World File" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;World File&lt;/p&gt;&lt;p&gt;File path&lt;/p&gt;</dialogReference>
      </param>
      <param name="NX" displayname="Number of Columns" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Number of Columns&lt;/p&gt;&lt;p&gt;Integer&lt;/p&gt;&lt;p&gt;Minimum: 1&lt;/p&gt;&lt;p&gt;Default: 100&lt;/p&gt;</dialogReference>
