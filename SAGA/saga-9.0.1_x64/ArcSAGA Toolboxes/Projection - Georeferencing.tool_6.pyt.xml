      <param name="NY" displayname="Number of Rows" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Number of Rows&lt;/p&gt;&lt;p&gt;Integer&lt;/p&gt;&lt;p&gt;Minimum: 1&lt;/p&gt;&lt;p&gt;Default: 100&lt;/p&gt;</dialogReference>
