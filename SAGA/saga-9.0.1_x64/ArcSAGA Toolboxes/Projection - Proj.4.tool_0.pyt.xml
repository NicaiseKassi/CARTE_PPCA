      </param>
      <param name="CRS_FILE" displayname="Well Known Text File" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Well Known Text File&lt;/p&gt;&lt;p&gt;File path&lt;/p&gt;</dialogReference>
      </param>
