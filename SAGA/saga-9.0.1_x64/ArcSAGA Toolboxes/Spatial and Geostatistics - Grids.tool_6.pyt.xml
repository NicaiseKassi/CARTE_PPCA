  <tool name="tool_6" displayname="Directional Statistics for Single Grid">
    <parameters>
      <param name="GRID" displayname="Grid" type="Required" direction="Input">
        <dialogReference>&lt;p&gt;Grid&lt;/p&gt;&lt;p&gt;Grid, input&lt;/p&gt;</dialogReference>
      </param>
      <param name="MEAN" displayname="Arithmetic Mean" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Arithmetic Mean&lt;/p&gt;&lt;p&gt;Grid, output, optional&lt;/p&gt;&lt;p&gt;optional&lt;/p&gt;</dialogReference>
      </param>
      <param name="DIFMEAN" displayname="Difference from Arithmetic Mean" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Difference from Arithmetic Mean&lt;/p&gt;&lt;p&gt;Grid, output, optional&lt;/p&gt;&lt;p&gt;optional&lt;/p&gt;</dialogReference>
      </param>
      <param name="MIN" displayname="Minimum" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Minimum&lt;/p&gt;&lt;p&gt;Grid, output, optional&lt;/p&gt;&lt;p&gt;optional&lt;/p&gt;</dialogReference>
      </param>
      <param name="MAX" displayname="Maximum" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Maximum&lt;/p&gt;&lt;p&gt;Grid, output, optional&lt;/p&gt;&lt;p&gt;optional&lt;/p&gt;</dialogReference>
      </param>
      <param name="RANGE" displayname="Range" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Range&lt;/p&gt;&lt;p&gt;Grid, output, optional&lt;/p&gt;&lt;p&gt;optional&lt;/p&gt;</dialogReference>
      </param>
      <param name="VAR" displayname="Variance" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Variance&lt;/p&gt;&lt;p&gt;Grid, output, optional&lt;/p&gt;&lt;p&gt;optional&lt;/p&gt;</dialogReference>
      </param>
      <param name="STDDEV" displayname="Standard Deviation" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Standard Deviation&lt;/p&gt;&lt;p&gt;Grid, output, optional&lt;/p&gt;&lt;p&gt;optional&lt;/p&gt;</dialogReference>
      </param>
      <param name="STDDEVLO" displayname="Mean less Standard Deviation" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Mean less Standard Deviation&lt;/p&gt;&lt;p&gt;Grid, output, optional&lt;/p&gt;&lt;p&gt;optional&lt;/p&gt;</dialogReference>
      </param>
      <param name="STDDEVHI" displayname="Mean plus Standard Deviation" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Mean plus Standard Deviation&lt;/p&gt;&lt;p&gt;Grid, output, optional&lt;/p&gt;&lt;p&gt;optional&lt;/p&gt;</dialogReference>
      </param>
      <param name="DEVMEAN" displayname="Deviation from Arithmetic Mean" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Deviation from Arithmetic Mean&lt;/p&gt;&lt;p&gt;Grid, output, optional&lt;/p&gt;&lt;p&gt;optional&lt;/p&gt;</dialogReference>
      </param>
      <param name="PERCENT" displayname="Percentile" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Percentile&lt;/p&gt;&lt;p&gt;Grid, output, optional&lt;/p&gt;&lt;p&gt;optional&lt;/p&gt;</dialogReference>
      </param>
      <param name="POINTS" displayname="Points" type="Optional" direction="Input">
        <dialogReference>&lt;p&gt;Points&lt;/p&gt;&lt;p&gt;Shapes, input, optional&lt;/p&gt;&lt;p&gt;optional&lt;/p&gt;</dialogReference>
      </param>
      <param name="POINTS_OUT" displayname="Directional Statistics for Points" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Directional Statistics for Points&lt;/p&gt;&lt;p&gt;Shapes, output, optional&lt;/p&gt;&lt;p&gt;optional&lt;/p&gt;</dialogReference>
      </param>
      <param name="DIRECTION" displayname="Direction [Degree]" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Direction [Degree]&lt;/p&gt;&lt;p&gt;Floating point&lt;/p&gt;&lt;p&gt;Default: 0.000000&lt;/p&gt;</dialogReference>
      </param>
      <param name="TOLERANCE" displayname="Tolerance [Degree]" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Tolerance [Degree]&lt;/p&gt;&lt;p&gt;Floating point&lt;/p&gt;&lt;p&gt;Minimum: 0.000000&lt;/p&gt;&lt;p&gt;Maximum: 45.000000&lt;/p&gt;&lt;p&gt;Default: 0.000000&lt;/p&gt;</dialogReference>
      </param>
      <param name="MAXDISTANCE" displayname="Maximum Distance [Cells]" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Maximum Distance [Cells]&lt;/p&gt;&lt;p&gt;Integer&lt;/p&gt;&lt;p&gt;Minimum: 0&lt;/p&gt;&lt;p&gt;Default: 0&lt;/p&gt;&lt;p&gt;Maximum distance parameter is ignored if set to zero (default).&lt;/p&gt;</dialogReference>
      </param>
      <param name="DW_WEIGHTING" displayname="Weighting Function" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Weighting Function&lt;/p&gt;&lt;p&gt;Choice&lt;/p&gt;&lt;p&gt;Available Choices:&lt;/p&gt;&lt;p&gt;[0] no distance weighting&lt;/p&gt;&lt;p&gt;[1] inverse distance to a power&lt;/p&gt;&lt;p&gt;[2] exponential&lt;/p&gt;&lt;p&gt;[3] gaussian&lt;/p&gt;&lt;p&gt;Default: 0&lt;/p&gt;</dialogReference>
      </param>
      <param name="DW_IDW_POWER" displayname="Power" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Power&lt;/p&gt;&lt;p&gt;Floating point&lt;/p&gt;&lt;p&gt;Minimum: 0.000000&lt;/p&gt;&lt;p&gt;Default: 2.000000&lt;/p&gt;</dialogReference>
      </param>
      <param name="DW_BANDWIDTH" displayname="Bandwidth" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Bandwidth&lt;/p&gt;&lt;p&gt;Floating point&lt;/p&gt;&lt;p&gt;Minimum: 0.000000&lt;/p&gt;&lt;p&gt;Default: 1.000000&lt;/p&gt;&lt;p&gt;Bandwidth specified as number of cells.&lt;/p&gt;</dialogReference>
      </param>
    </parameters>
    <summary>Calculates for each cell statistical properties (arithmetic mean, minimum, maximum, variance, standard deviation) of all cells lying in given direction based on the input grid. </summary>
  </tool>
