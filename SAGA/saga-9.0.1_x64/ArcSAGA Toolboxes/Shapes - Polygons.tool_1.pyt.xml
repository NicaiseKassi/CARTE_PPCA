  <tool name="tool_1" displayname="Polygon Properties">
    <parameters>
      <param name="POLYGONS" displayname="Polygons" type="Required" direction="Input">
        <dialogReference>&lt;p&gt;Polygons&lt;/p&gt;&lt;p&gt;Shapes, input&lt;/p&gt;</dialogReference>
      </param>
