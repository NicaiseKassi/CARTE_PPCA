  <tool name="tool_6" displayname="Support Vector Machine Classification (OpenCV)">
    <parameters>
      <param name="FEATURES" displayname="Features" type="Required" direction="Input">
        <dialogReference>&lt;p&gt;Features&lt;/p&gt;&lt;p&gt;Grid list, input&lt;/p&gt;</dialogReference>
      </param>
      <param name="NORMALIZE" displayname="Normalize" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Normalize&lt;/p&gt;&lt;p&gt;Boolean&lt;/p&gt;&lt;p&gt;Default: 0&lt;/p&gt;</dialogReference>
      </param>
      <param name="CLASSES" displayname="Classification" type="Required" direction="Output">
        <dialogReference>&lt;p&gt;Classification&lt;/p&gt;&lt;p&gt;Grid, output&lt;/p&gt;</dialogReference>
      </param>
      <param name="CLASSES_LUT" displayname="Look-up Table" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Look-up Table&lt;/p&gt;&lt;p&gt;Table, output, optional&lt;/p&gt;&lt;p&gt;optional&lt;/p&gt;&lt;p&gt;A reference list of the grid values that have been assigned to the training classes.&lt;/p&gt;</dialogReference>
      </param>
      <param name="MODEL_LOAD" displayname="Load Model" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Load Model&lt;/p&gt;&lt;p&gt;File path&lt;/p&gt;&lt;p&gt;Use a model previously stored to file.&lt;/p&gt;</dialogReference>
      </param>
      <param name="TRAIN_AREAS" displayname="Training Areas" type="Required" direction="Input">
        <dialogReference>&lt;p&gt;Training Areas&lt;/p&gt;&lt;p&gt;Shapes, input&lt;/p&gt;</dialogReference>
      </param>
      <param name="TRAIN_CLASS" displayname="Class Identifier" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Class Identifier&lt;/p&gt;&lt;p&gt;Table field&lt;/p&gt;</dialogReference>
      </param>
      <param name="TRAIN_BUFFER" displayname="Buffer Size" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Buffer Size&lt;/p&gt;&lt;p&gt;Floating point&lt;/p&gt;&lt;p&gt;Minimum: 0.000000&lt;/p&gt;&lt;p&gt;Default: 1.000000&lt;/p&gt;&lt;p&gt;For non-polygon type training areas, creates a buffer with a diameter of specified size.&lt;/p&gt;</dialogReference>
      </param>
      <param name="MODEL_SAVE" displayname="Save Model" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Save Model&lt;/p&gt;&lt;p&gt;File path&lt;/p&gt;&lt;p&gt;Stores model to file to be used for subsequent classifications instead of training areas.&lt;/p&gt;</dialogReference>
      </param>
      <param name="SVM_TYPE" displayname="SVM Type" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;SVM Type&lt;/p&gt;&lt;p&gt;Choice&lt;/p&gt;&lt;p&gt;Available Choices:&lt;/p&gt;&lt;p&gt;[0] c-support vector classification&lt;/p&gt;&lt;p&gt;[1] nu support vector classification&lt;/p&gt;&lt;p&gt;[2] distribution estimation (one class)&lt;/p&gt;&lt;p&gt;[3] epsilon support vector regression&lt;/p&gt;&lt;p&gt;[4] nu support vector regression&lt;/p&gt;&lt;p&gt;Default: 0&lt;/p&gt;</dialogReference>
      </param>
      <param name="C" displayname="C" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;C&lt;/p&gt;&lt;p&gt;Floating point&lt;/p&gt;&lt;p&gt;Minimum: 0.000000&lt;/p&gt;&lt;p&gt;Default: 1.000000&lt;/p&gt;</dialogReference>
      </param>
      <param name="NU" displayname="Nu" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Nu&lt;/p&gt;&lt;p&gt;Floating point&lt;/p&gt;&lt;p&gt;Minimum: 0.000000&lt;/p&gt;&lt;p&gt;Default: 0.500000&lt;/p&gt;</dialogReference>
      </param>
      <param name="P" displayname="P" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;P&lt;/p&gt;&lt;p&gt;Floating point&lt;/p&gt;&lt;p&gt;Minimum: 0.000000&lt;/p&gt;&lt;p&gt;Default: 0.500000&lt;/p&gt;</dialogReference>
      </param>
      <param name="KERNEL" displayname="Kernel Type" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Kernel Type&lt;/p&gt;&lt;p&gt;Choice&lt;/p&gt;&lt;p&gt;Available Choices:&lt;/p&gt;&lt;p&gt;[0] linear&lt;/p&gt;&lt;p&gt;[1] polynomial&lt;/p&gt;&lt;p&gt;[2] radial basis function&lt;/p&gt;&lt;p&gt;[3] sigmoid&lt;/p&gt;&lt;p&gt;[4] exponential chi2&lt;/p&gt;&lt;p&gt;[5] histogram intersection&lt;/p&gt;&lt;p&gt;Default: 1&lt;/p&gt;</dialogReference>
      </param>
      <param name="COEF0" displayname="Coefficient 0" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Coefficient 0&lt;/p&gt;&lt;p&gt;Floating point&lt;/p&gt;&lt;p&gt;Minimum: 0.000000&lt;/p&gt;&lt;p&gt;Default: 1.000000&lt;/p&gt;</dialogReference>
      </param>
      <param name="DEGREE" displayname="Degree" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Degree&lt;/p&gt;&lt;p&gt;Floating point&lt;/p&gt;&lt;p&gt;Minimum: 0.000000&lt;/p&gt;&lt;p&gt;Default: 0.500000&lt;/p&gt;</dialogReference>
      </param>
      <param name="GAMMA" displayname="Gamma" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Gamma&lt;/p&gt;&lt;p&gt;Floating point&lt;/p&gt;&lt;p&gt;Minimum: 0.000000&lt;/p&gt;&lt;p&gt;Default: 1.000000&lt;/p&gt;</dialogReference>
      </param>
    </parameters>
    <summary>Integration of the OpenCV Machine Learning library for Support Vector Machine classification of gridded features.</summary>
  </tool>
