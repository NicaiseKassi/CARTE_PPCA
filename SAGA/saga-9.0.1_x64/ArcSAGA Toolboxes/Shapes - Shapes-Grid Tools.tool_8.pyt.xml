  <tool name="tool_8" displayname="Grid Statistics for Points">
    <parameters>
      <param name="GRIDS" displayname="Grids" type="Required" direction="Input">
        <dialogReference>&lt;p&gt;Grids&lt;/p&gt;&lt;p&gt;Grid list, input&lt;/p&gt;</dialogReference>
      </param>
      <param name="POINTS" displayname="Points" type="Required" direction="Input">
        <dialogReference>&lt;p&gt;Points&lt;/p&gt;&lt;p&gt;Shapes, input&lt;/p&gt;</dialogReference>
      </param>
      <param name="KERNEL_TYPE" displayname="Kernel Type" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Kernel Type&lt;/p&gt;&lt;p&gt;Choice&lt;/p&gt;&lt;p&gt;Available Choices:&lt;/p&gt;&lt;p&gt;[0] square&lt;/p&gt;&lt;p&gt;[1] circle&lt;/p&gt;&lt;p&gt;Default: 0&lt;/p&gt;</dialogReference>
      </param>
      <param name="KERNEL_SIZE" displayname="Kernel Size" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Kernel Size&lt;/p&gt;&lt;p&gt;Integer&lt;/p&gt;&lt;p&gt;Minimum: 1&lt;/p&gt;&lt;p&gt;Default: 1&lt;/p&gt;&lt;p&gt;kernel size defined as radius number of cells&lt;/p&gt;</dialogReference>
      </param>
      <param name="NAMING" displayname="Field Naming" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Field Naming&lt;/p&gt;&lt;p&gt;Choice&lt;/p&gt;&lt;p&gt;Available Choices:&lt;/p&gt;&lt;p&gt;[0] grid number&lt;/p&gt;&lt;p&gt;[1] grid name&lt;/p&gt;&lt;p&gt;Default: 1&lt;/p&gt;</dialogReference>
      </param>
      <param name="RESULT" displayname="Statistics" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Statistics&lt;/p&gt;&lt;p&gt;Shapes, output, optional&lt;/p&gt;&lt;p&gt;optional&lt;/p&gt;</dialogReference>
      </param>
      <param name="COUNT" displayname="Number of Cells" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Number of Cells&lt;/p&gt;&lt;p&gt;Boolean&lt;/p&gt;&lt;p&gt;Default: 1&lt;/p&gt;</dialogReference>
      </param>
      <param name="MIN" displayname="Minimum" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Minimum&lt;/p&gt;&lt;p&gt;Boolean&lt;/p&gt;&lt;p&gt;Default: 1&lt;/p&gt;</dialogReference>
      </param>
      <param name="MAX" displayname="Maximum" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Maximum&lt;/p&gt;&lt;p&gt;Boolean&lt;/p&gt;&lt;p&gt;Default: 1&lt;/p&gt;</dialogReference>
      </param>
      <param name="RANGE" displayname="Range" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Range&lt;/p&gt;&lt;p&gt;Boolean&lt;/p&gt;&lt;p&gt;Default: 1&lt;/p&gt;</dialogReference>
      </param>
      <param name="SUM" displayname="Sum" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Sum&lt;/p&gt;&lt;p&gt;Boolean&lt;/p&gt;&lt;p&gt;Default: 1&lt;/p&gt;</dialogReference>
      </param>
      <param name="MEAN" displayname="Mean" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Mean&lt;/p&gt;&lt;p&gt;Boolean&lt;/p&gt;&lt;p&gt;Default: 1&lt;/p&gt;</dialogReference>
      </param>
      <param name="VAR" displayname="Variance" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Variance&lt;/p&gt;&lt;p&gt;Boolean&lt;/p&gt;&lt;p&gt;Default: 1&lt;/p&gt;</dialogReference>
      </param>
      <param name="STDDEV" displayname="Standard Deviation" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Standard Deviation&lt;/p&gt;&lt;p&gt;Boolean&lt;/p&gt;&lt;p&gt;Default: 1&lt;/p&gt;</dialogReference>
      </param>
      <param name="QUANTILE" displayname="Percentile" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Percentile&lt;/p&gt;&lt;p&gt;Integer&lt;/p&gt;&lt;p&gt;Minimum: 0&lt;/p&gt;&lt;p&gt;Maximum: 50&lt;/p&gt;&lt;p&gt;Default: 0&lt;/p&gt;&lt;p&gt;Calculate distribution percentiles. Value specifies interval (median=50, quartiles=25, deciles=10, ...). Set to zero to omit percentile calculation.&lt;/p&gt;</dialogReference>
      </param>
    </parameters>
    <summary>For each given point statistics based on all grid cells in the defined neighbourhood will be calculated.</summary>
  </tool>
