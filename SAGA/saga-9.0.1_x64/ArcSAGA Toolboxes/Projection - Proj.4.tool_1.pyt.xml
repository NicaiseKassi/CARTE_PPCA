      </param>
      <param name="TRANSFORM_Z" displayname="Z Transformation" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Z Transformation&lt;/p&gt;&lt;p&gt;Boolean&lt;/p&gt;&lt;p&gt;Default: 1&lt;/p&gt;&lt;p&gt;Transform elevation (z) values, if appropriate.&lt;/p&gt;</dialogReference>
      </param>
      <param name="PARALLEL" displayname="Parallel Processing" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Parallel Processing&lt;/p&gt;&lt;p&gt;Boolean&lt;/p&gt;&lt;p&gt;Default: 0&lt;/p&gt;</dialogReference>
      </param>
