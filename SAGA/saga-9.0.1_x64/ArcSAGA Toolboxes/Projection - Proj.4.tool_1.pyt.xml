      </param>
      <param name="CRS_FILE" displayname="Well Known Text File" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Well Known Text File&lt;/p&gt;&lt;p&gt;File path&lt;/p&gt;</dialogReference>
      </param>
      <param name="CRS_EPSG" displayname="Authority Code" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Authority Code&lt;/p&gt;&lt;p&gt;Integer&lt;/p&gt;&lt;p&gt;Default: -1&lt;/p&gt;</dialogReference>
