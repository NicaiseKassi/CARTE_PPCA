        <dialogReference>&lt;p&gt;Graticule&lt;/p&gt;&lt;p&gt;Shapes, output&lt;/p&gt;</dialogReference>
      </param>
      <param name="GRATICULE_RECT" displayname="Graticule" type="Required" direction="Output">
        <dialogReference>&lt;p&gt;Graticule&lt;/p&gt;&lt;p&gt;Shapes, output&lt;/p&gt;</dialogReference>
      </param>
      <param name="TYPE" displayname="Type" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Type&lt;/p&gt;&lt;p&gt;Choice&lt;/p&gt;&lt;p&gt;Available Choices:&lt;/p&gt;&lt;p&gt;[0] Lines&lt;/p&gt;&lt;p&gt;[1] Rectangles&lt;/p&gt;&lt;p&gt;Default: 0&lt;/p&gt;</dialogReference>
