        <dialogReference>&lt;p&gt;Extent&lt;/p&gt;&lt;p&gt;Shapes, input, optional&lt;/p&gt;&lt;p&gt;optional&lt;/p&gt;</dialogReference>
      </param>
      <param name="EXTENT_X_MIN" displayname="Minimum" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Minimum&lt;/p&gt;&lt;p&gt;Floating point&lt;/p&gt;&lt;p&gt;Default: -180.000000&lt;/p&gt;</dialogReference>
