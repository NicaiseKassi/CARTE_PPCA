      <param name="OUTPUT" displayname="Referenced Grids" type="Required" direction="Output">
        <dialogReference>&lt;p&gt;Referenced Grids&lt;/p&gt;&lt;p&gt;Grid list, output&lt;/p&gt;</dialogReference>
      </param>
      <param name="EXTENT" displayname="Extent" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Extent&lt;/p&gt;&lt;p&gt;Shapes, output, optional&lt;/p&gt;&lt;p&gt;optional&lt;/p&gt;</dialogReference>
      </param>
      <param name="DEM" displayname="Elevation" type="Optional" direction="Input">
