      </param>
      <param name="STDDEV" displayname="Standard Deviation" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Standard Deviation&lt;/p&gt;&lt;p&gt;Boolean&lt;/p&gt;&lt;p&gt;Default: 1&lt;/p&gt;</dialogReference>
