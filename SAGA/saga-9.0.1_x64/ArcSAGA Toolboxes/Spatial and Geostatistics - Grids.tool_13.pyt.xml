      </param>
      <param name="STATS" displayname="Statistics for Grids" type="Required" direction="Output">
        <dialogReference>&lt;p&gt;Statistics for Grids&lt;/p&gt;&lt;p&gt;Table, output&lt;/p&gt;</dialogReference>
      </param>
      <param name="DATA_CELLS" displayname="Number of Data Cells" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Number of Data Cells&lt;/p&gt;&lt;p&gt;Boolean&lt;/p&gt;&lt;p&gt;Default: 0&lt;/p&gt;</dialogReference>
      </param>
      <param name="NODATA_CELLS" displayname="Number of No-Data Cells" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Number of No-Data Cells&lt;/p&gt;&lt;p&gt;Boolean&lt;/p&gt;&lt;p&gt;Default: 0&lt;/p&gt;</dialogReference>
      </param>
