      </param>
      <param name="STDDEVHI" displayname="Mean plus Standard Deviation" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Mean plus Standard Deviation&lt;/p&gt;&lt;p&gt;Boolean&lt;/p&gt;&lt;p&gt;Default: 0&lt;/p&gt;</dialogReference>
