  <tool name="tool_20" displayname="Unique Value Statistics for Grids">
    <parameters>
      <param name="GRIDS" displayname="Values" type="Required" direction="Input">
        <dialogReference>&lt;p&gt;Values&lt;/p&gt;&lt;p&gt;Grid list, input&lt;/p&gt;</dialogReference>
      </param>
      <param name="MAJORITY" displayname="Majority" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Majority&lt;/p&gt;&lt;p&gt;Grid, output, optional&lt;/p&gt;&lt;p&gt;optional&lt;/p&gt;</dialogReference>
