  <tool name="tool_7" displayname="GWR for Multiple Predictors (Shapes)">
    <parameters>
      <param name="POINTS" displayname="Shapes" type="Required" direction="Input">
        <dialogReference>&lt;p&gt;Shapes&lt;/p&gt;&lt;p&gt;Shapes, input&lt;/p&gt;</dialogReference>
      </param>
      <param name="DEPENDENT" displayname="Dependent Variable" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Dependent Variable&lt;/p&gt;&lt;p&gt;Table field&lt;/p&gt;</dialogReference>
      </param>
      <param name="PREDICTORS" displayname="Predictors" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Predictors&lt;/p&gt;&lt;p&gt;Table fields&lt;/p&gt;</dialogReference>
      </param>
      <param name="REGRESSION" displayname="Regression" type="Required" direction="Output">
        <dialogReference>&lt;p&gt;Regression&lt;/p&gt;&lt;p&gt;Shapes, output&lt;/p&gt;</dialogReference>
      </param>
      <param name="LOGISTIC" displayname="Logistic Regression" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Logistic Regression&lt;/p&gt;&lt;p&gt;Boolean&lt;/p&gt;&lt;p&gt;Default: 0&lt;/p&gt;</dialogReference>
      </param>
      <param name="DW_WEIGHTING" displayname="Weighting Function" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Weighting Function&lt;/p&gt;&lt;p&gt;Choice&lt;/p&gt;&lt;p&gt;Available Choices:&lt;/p&gt;&lt;p&gt;[0] no distance weighting&lt;/p&gt;&lt;p&gt;[1] inverse distance to a power&lt;/p&gt;&lt;p&gt;[2] exponential&lt;/p&gt;&lt;p&gt;[3] gaussian&lt;/p&gt;&lt;p&gt;Default: 0&lt;/p&gt;</dialogReference>
      </param>
      <param name="DW_IDW_POWER" displayname="Power" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Power&lt;/p&gt;&lt;p&gt;Floating point&lt;/p&gt;&lt;p&gt;Minimum: 0.000000&lt;/p&gt;&lt;p&gt;Default: 2.000000&lt;/p&gt;</dialogReference>
      </param>
      <param name="DW_BANDWIDTH" displayname="Bandwidth" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Bandwidth&lt;/p&gt;&lt;p&gt;Floating point&lt;/p&gt;&lt;p&gt;Minimum: 0.000000&lt;/p&gt;&lt;p&gt;Default: 1.000000&lt;/p&gt;&lt;p&gt;Bandwidth for exponential and Gaussian weighting&lt;/p&gt;</dialogReference>
      </param>
      <param name="SEARCH_RANGE" displayname="Search Range" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Search Range&lt;/p&gt;&lt;p&gt;Choice&lt;/p&gt;&lt;p&gt;Available Choices:&lt;/p&gt;&lt;p&gt;[0] local&lt;/p&gt;&lt;p&gt;[1] global&lt;/p&gt;&lt;p&gt;Default: 1&lt;/p&gt;</dialogReference>
      </param>
      <param name="SEARCH_RADIUS" displayname="Maximum Search Distance" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Maximum Search Distance&lt;/p&gt;&lt;p&gt;Floating point&lt;/p&gt;&lt;p&gt;Minimum: 0.000000&lt;/p&gt;&lt;p&gt;Default: 1000.000000&lt;/p&gt;&lt;p&gt;local maximum search distance given in map units&lt;/p&gt;</dialogReference>
      </param>
      <param name="SEARCH_POINTS_ALL" displayname="Number of Points" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Number of Points&lt;/p&gt;&lt;p&gt;Choice&lt;/p&gt;&lt;p&gt;Available Choices:&lt;/p&gt;&lt;p&gt;[0] maximum number of nearest points&lt;/p&gt;&lt;p&gt;[1] all points within search distance&lt;/p&gt;&lt;p&gt;Default: 1&lt;/p&gt;</dialogReference>
      </param>
      <param name="SEARCH_POINTS_MIN" displayname="Minimum" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Minimum&lt;/p&gt;&lt;p&gt;Integer&lt;/p&gt;&lt;p&gt;Minimum: 1&lt;/p&gt;&lt;p&gt;Default: 16&lt;/p&gt;&lt;p&gt;minimum number of points to use&lt;/p&gt;</dialogReference>
      </param>
      <param name="SEARCH_POINTS_MAX" displayname="Maximum" type="Optional" direction="Output">
        <dialogReference>&lt;p&gt;Maximum&lt;/p&gt;&lt;p&gt;Integer&lt;/p&gt;&lt;p&gt;Minimum: 1&lt;/p&gt;&lt;p&gt;Default: 20&lt;/p&gt;&lt;p&gt;maximum number of nearest points&lt;/p&gt;</dialogReference>
      </param>
    </parameters>
    <summary>Geographically Weighted Regression for multiple predictors. Regression details are stored in a copy of the input data set. If the input data set is not a point data set, the feature centroids are used as spatial reference. </summary>
  </tool>
